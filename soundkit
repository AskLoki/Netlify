      			<script>
				var button1 = document.getElementById('button1');
				var button1Btn = document.getElementById('button1-btn')			
				button1Btn.addEventListener('mouseenter', function(){
				   button1Btn.classList.add('playing');
				   playPause(button1);
				});				
				button1Btn.addEventListener('click', function(){
				   button1Btn.classList.add('playing');
				   playPause(button1);
				});		
				button1Btn.addEventListener('mouseleave', function(){
					button1Btn.classList.remove('playing');
					stopPlaying(button1);
				});
				</script>
    			
